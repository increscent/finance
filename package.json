{
    "name": "finance",
    "version": "1.0.0",
    "description": "",
    "main": "api/index.js",
    "scripts": {
        "test": "npm run build && mocha api_build/test/integrationTests.js",
        "build": "browserify app/main.js -t babelify -t node-lessify -g uglifyify -o public/bundle.js && babel api -d api_build && chmod +x api_build/index.js",
        "start": "npm run build && node api_build/index.js",
        "watch-less": "watchify app/main.less -t node-lessify -o public/style.js -v",
        "watch-react": "watchify app/main.js -t babelify -o public/bundle.js -v",
        "watch-server": "nodemon api/index.js --watch api --exec babel-node"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@bitbucket.org/increscent/finance.git"
    },
    "author": "Robert Williams",
    "license": "ISC",
    "homepage": "https://bitbucket.org/increscent/finance#readme",
    "dependencies": {
        "babel-cli": "^6.0.0",
        "babel-core": "^6.0.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babelify": "^8.0.0",
        "body-parser": "^1.18.2",
        "browserify": "^16.2.3",
        "classnames": "^2.2.5",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "^1.4.3",
        "expect": "^24.1.0",
        "express": "^4.16.2",
        "express-session": "^1.15.6",
        "mixin": "^0.2.0",
        "mocha": "^6.0.2",
        "mongoose": "^5.4.17",
        "node-lessify": "^0.1.5",
        "nodemon": "^1.14.7",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-fastclick": "^3.0.2",
        "react-redux": "^6.0.1",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "redux": "^4.0.1",
        "redux-devtools": "^3.4.1",
        "uglifyify": "^5.0.2"
    },
    "babel": {
        "plugins": [
            [
                "transform-object-rest-spread",
                {
                    "useBuiltIns": true
                }
            ]
        ],
        "presets": [
            "env",
            "stage-2",
            "react"
        ]
    }
}
